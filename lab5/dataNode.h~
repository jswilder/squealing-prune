#ifndef DATA_NODE_H
#define DATA_NODE_H

#include <string>
using namespace std;

class DataNode {
  private:
    const string element;
    DataNode *next;
  public:
    DataNode(const string &s, DataNode *n) 
                     : element(s) , next(n)
    {
    }
  friend class GiggleTree;
  friend class GatewayNode;  
};

#endif
