#include <fstream>
#include <iostream>
using namespace std;
 
int main (){

    using namespace std;

    ifstream file("file.txt");
    if(file.is_open())
    {
        string myArray[100];

        for(int i = 0; i < 100; ++i)
        {
            file >> myArray[i];
        }
    }

return 0;
}
